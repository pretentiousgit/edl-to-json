{
  "env": {
    "node": true,
    "commonjs": true
  },
  "globals": {
    "require": true
  },
  "extends": [
    "react-app"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "global-require": 0,
    "quotes": [
      1,
      "single"
    ],
    "indent": [
      1,
      2,
      {
        "MemberExpression": 1
      }
    ],
    "linebreak-style": [
      1,
      "unix"
    ],
    "newline-per-chained-call": [
      "error",
      {
        "ignoreChainWithDepth": 2
      }
    ],
    "semi": [
      1,
      "always"
    ],
    "eol-last": [
      0
    ],
    "comma-spacing": 1,
    "arrow-parens": [
      1,
      "always"
    ],
    "no-mixed-requires": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "max-len": [
      "error",
      100
    ],
    "no-extra-parens": 0,
    "no-console": "off",
    "brace-style": [
      "error",
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "curly": "error",
    "object-curly-spacing": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-trailing-spaces": 1,
    "no-unused-vars": 1,
    "key-spacing": 1,
    "comma-dangle": [
      1,
      "never"
    ],
    "space-before-function-paren": 0,
    "object-shorthand": 0,
    "space-infix-ops": 1,
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}